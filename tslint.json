{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": {
            "options": [
                120
            ]
        },
        "interface-name": false,
        "object-literal-sort-keys": false,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules"
        ]
    }
}
